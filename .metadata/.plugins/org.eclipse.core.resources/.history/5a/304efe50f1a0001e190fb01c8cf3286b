package com.bridgelabz.linecompariosn;

public class LineComparison {
		
		Line1 l1;
		Line2 l2;
		 
		public void byEquals() {
			 
			Line1 l1 = new Line1();  
			Line2 l2 = new Line2(); 
					
					if(l1.line1.equals(l2.line2)){
						System.out.println("Lenghth of line1 and line2 are Equal");
					}else System.out.println("Lenghth of line1 and line2 are Not Equal");
		
	
	
		
	}
}
