package com.bridgelabz.linecompariosn;

public class Line {
	double x1 = 5;  double y1 = 5;
	double x2 = 10; double y2 = 10;
	double x = (x2-x1)*(x2-x1);
	double y = (y2-y1)*(y2-y1);
	double z = x + y;
	double t;  
	double line1= z/2;  
	do{  
		t=line1;  
		line1=(t+(z/t))/2;  
		}   
	while((t-line1)!= 0);  
			System.out.println("1st Length Beetween Two Points= "+ line1);
	

}
